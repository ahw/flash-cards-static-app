{
  "extends": "airbnb",
  "globals": {
    "__DEV__": true,
    "MathJax": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "comma-dangle": 1,
    "prefer-const": 1,
    "no-unused-vars": 1,
    "no-else-return": 1,
    "no-param-reassign": 1,
    "quotes": 1,
    "semi": 1,
    "no-undef": 1,
    "no-inner-declarations": 1,
    "react/prop-types": 1,
    "react/wrap-multilines": 1,
    "no-multi-spaces": 1,
    "key-spacing": 1,
    "space-before-function-paren": 1,
    "indent": ["warn", 4],
    "id-length": 1,
    "no-regex-spaces": 1,
    "no-trailing-spaces": 1,
    "space-after-keywords": 1,
    "padded-blocks": 1,
    "no-shadow": 1,
    "space-infix-ops": 1,
    "no-use-before-define": 1,
    "no-vars": 1,
    "no-var": 1,
    "vars-on-top": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "no-empty": 1,
    "eqeqeq": 1,
    "consistent-return": 1,
    "react/sort-comp": 1,
    "new-cap": 1,
    "space-infix-ops": 1,
    "jsx-quotes": 1,
    "react/no-unknown-property": 1,
  }
}
